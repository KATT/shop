{
  "name": "@katt/shop-web",
  "private": true,
  "version": "1.0.4-0",
  "scripts": {
    "dev": "dotenv -- nodemon server/index.ts",
    "build": "tsc --project tsconfig.server.json && dotenv -- next build",
    "start": "NODE_ENV=production dotenv -- node dist/",
    "deploy": "now -e GRAPHQL_URL=https://shop-api.kattcorp.co.uk/",
    "alias": "now alias"
  },
  "engines": {
    "node": "9.x.x"
  },
  "dependencies": {
    "@katt/shop-api": "^1.0.4-0",
    "apollo-client-preset": "^1.0.8",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "graphql-anywhere": "^4.1.5",
    "graphql-tag": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-modal": "^3.3.1",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.12.4",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/lodash": "^4.14.104",
    "@types/lodash.flowright": "^3.5.3",
    "@types/next": "^2.4.8",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.38",
    "@types/react-modal": "^3.1.2",
    "@zeit/next-typescript": "0.0.10",
    "concurrently": "^3.5.1",
    "dotenv-cli": "^1.4.0",
    "nodemon": "^1.17.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  },
  "now": {
    "alias": [
      "react-prisma-graphql-shopping-cart.kattcorp.co.uk",
      "react-prisma-graphql-shopping-cart.kattcorp.com",
      "shop.kattcorp.co.uk",
      "shop.kattcorp.com"
    ]
  }
}
